<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".fragments.RateFragment">

    <RatingBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ratingBarAfter"
            android:layout_gravity="center"

            android:theme="@style/RatingBar"/>
    <TextView
            android:text="Oceń swój przejazd"
            android:textColor="@color/colorPrimary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView" android:textSize="21sp"
            android:textStyle="bold" android:layout_marginTop="200dp" android:layout_marginLeft="100dp"/>

</FrameLayout>