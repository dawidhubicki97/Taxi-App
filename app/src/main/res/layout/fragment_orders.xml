<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent" xmlns:app="http://schemas.android.com/apk/res-auto"
             tools:context=".fragments.OrdersFragment">


    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="626dp" android:id="@+id/ordersRecyclerView"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" android:layout_marginTop="100dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ratingOrdersTextView"
            android:layout_marginTop="80dp"/>
</FrameLayout>